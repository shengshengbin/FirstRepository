<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.kgc.tangcco.appinfosys.dao.devuser.DevUserMapper">
    <!--根据用户编码查询用户-->
    <select id="getUserByCode" parameterType="string" resultType="DevUser">
        select * from dev_user where devCode = #{userCode}
    </select>


</mapper>